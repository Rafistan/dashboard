{
    "name": "rafi-step",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^2.6.5",
        "google-images": "^2.1.0",
        "moment": "^2.24.0",
        "numeral": "^2.0.6",
        "vue": "^2.6.10",
        "vue-carousel-3d": "^0.2.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-plugin-eslint": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "@vue/eslint-config-airbnb": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "5.0.0",
        "sass-loader": "10.1.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/airbnb"
        ],
        "rules": {
            "indent": 0,
            "no-confusing-arrow": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "overrides": [
            {
                "files": [
                    "**/icons/**/*.vue"
                ],
                "rules": {
                    "max-len": [
                        "error",
                        {
                            "code": 1000
                        }
                    ]
                }
            }
        ]
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
